@ARTICLE{GudysAdamandSikoraMarekandWrobelLukasz:Separateandconquerheuristicallowsrobustminingofcontrastsetsinclassificationregressionandsurvivaldata2024,
  author = {Gudyś, Adam and Sikora, Marek and Wróbel, Łukasz},
