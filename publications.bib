@ARTICLE{RuszczakBogdanandKotowskiKrzysztofandEvansDavidandNalepaJakub:TheOPS-SATbenchmarkfordetectinganomaliesinsatellitetelemetry2025,
  author = {Ruszczak, Bogdan and Kotowski, Krzysztof and Evans, David and Nalepa,
