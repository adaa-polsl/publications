@ARTICLE{FerlinMariaandMajchrowskaSylwiaandPlantykowMartaandKwasniewskaAlicjaandMikolajczyk-BarelaAgnieszkaandOlechMilenaandNalepaJakub:QuantifyinginconsistenciesintheHamburgSignLanguageNotationSystem2024,
  author = {Ferlin, Maria and Majchrowska, Sylwia and Plantykow, Marta and Kwaśniewska,
