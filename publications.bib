@Inbook{Stanczyk2019,
	author="Sta{\'{n}}czyk, Urszula",
	title="Weighting Attributes and Decision Rules Through Rankings and Discretisation Parameters",
	bookTitle="Machine Learning Paradigms: Theory and Application",
