}


@INPROCEEDINGS{ZiajaMaciejandKowaleczkoPawelandNalepaJakubandKostrzewaDanielandLatiniDanieleandDeSantisDavideandSalvucciGiorgiaandPetraccaIlariaandPegnaValeriaLaandDelFrateFabioandKawulokMichal:Hyperspectralimagepansharpening:thePrismacasestudy2023,
  author = {Ziaja, Maciej and Kowaleczko, Pawel and Nalepa, Jakub and Kostrzewa,
	Daniel and Latini, Daniele and De Santis, Davide and Salvucci, Giorgia
	and Petracca, Ilaria and Pegna, Valeria La and Del Frate, Fabio and
