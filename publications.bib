@INPROCEEDINGS{TarasiewiczTomaszandKawulokMichal:Graph-basedrepresentationformulti-imagesuper-resolution2023,
  author = {Tarasiewicz, Tomasz and Kawulok, Michał},
  title = {Graph-based representation for multi-image super-resolution},
