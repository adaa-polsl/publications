@INPROCEEDINGS{WijataAgataandPycinskiBartlomiejandNalepaJakub:Aneedleinamedicalhaystack:detectingabiopsyneedleinultrasoundimagesusingvisiontransformers2024,
  author = {Wijata, Agata and Pyciński, Bartłomiej and Nalepa, Jakub},
  title = {A needle in a (medical) haystack: detecting a biopsy needle in ultrasound
